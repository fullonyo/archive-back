{
  "name": "archive-nyo-backend",
  "version": "1.0.0",
  "description": "Backend API para Archive Nyo",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "migrate": "node scripts/migrate.js",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "node scripts/seed.js",
    "db:setup": "npm run prisma:generate && npm run prisma:push && npm run prisma:seed",
    "docker:build": "docker build -t vrchieve-backend .",
    "docker:run": "docker run -p 3001:3001 --env-file .env vrchieve-backend",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build -d",
    "docker:prod-lite": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build --build-arg DOCKERFILE=Dockerfile.lite && docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:prod-simple": "docker-compose -f docker-compose.yml -f docker-compose.lite.yml up --build -d",
    "docker:prod-rebuild": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build --no-cache && docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:ssl": "docker-compose -f docker-compose.yml -f docker-compose.ssl.yml up --build -d",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v --remove-orphans && docker system prune -f",
    "docker:logs": "docker-compose logs -f backend",
    "docker:logs-nginx": "docker-compose logs -f nginx",
    "docker:logs-redis": "docker-compose logs -f redis",
    "docker:redis": "docker-compose exec redis redis-cli -a ${REDIS_PASSWORD:-vrchieve123}",
    "docker:shell": "docker-compose exec backend sh",
    "docker:nginx-shell": "docker-compose exec nginx sh",
    "docker:nginx-reload": "docker-compose exec nginx nginx -s reload",
    "docker:nginx-test": "docker-compose exec nginx nginx -t",
    "docker:ps": "docker-compose ps",
    "docker:stats": "docker stats $(docker-compose ps -q)",
    "ssl:dev": "cd nginx && chmod +x generate-ssl-dev.sh && ./generate-ssl-dev.sh",
    "ssl:prod": "cd nginx && chmod +x setup-ssl-prod.sh && ./setup-ssl-prod.sh"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "etag": "^1.8.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "mysql2": "^3.6.5",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "redis": "^5.8.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^6.15.0",
    "supertest": "^7.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
